// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

model Pax {
  id         Int      @id @default(autoincrement())
  firstname  String
  middlename String
  lastname   String
  email      String   @unique
  dob        DateTime
  obs        String
}
